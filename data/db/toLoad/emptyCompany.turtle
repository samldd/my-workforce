default:Employee rdfs:subClassOf foaf:Person.

default:Employee
  rdfs:subClassOf esco:Person;
  default:worksFor "Mycompany"@en.

default:EmployeeSkill
  rdfs:label "Description of an skill of a particular employee.".

default:EmployeeFunction
  rdfs:label "Description of an function of a particular employee.".
